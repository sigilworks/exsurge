{
    "name": "exsurge",
    "version": "1.0.0",
    "description": "A JavaScript library for rendering Gregorian Chant in square note notation",
    "main": "dist/exsurge.js",
    "scripts": {
        "clean": "rm -rf ./dist/*",
        "build": "webpack",
        "build:watch": "NODE_ENV=development watch --ignoreDirectoryPattern=/dist/ webpack ./src",
        "test": "NODE_ENV=test jest --config=jest.config.js",
        "test:watch": "NODE_ENV=test jest --config=jest.config.js --watch",
        "test:coverage": "NODE_ENV=test jest --config=jest.coverage.config.js",
        "lint": "npm run lint:js",
        "lint:js": "eslint src/ test/",
        "lint:js:fix": "eslint --fix src/ test/",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/frmatthew/exsurge.git"
    },
    "author": "Fr. Matthew Spencer, O.S.J. <mspencer@osjusa.org> (http://marello.org)",
    "contributors": [
        "Fr. Matthew Spencer, O.S.J. <mspencer@osjusa.org> (http://marello.org)",
        "Timothy J. Palmer <tim@sigilworks.com> (http://www.sigilworks.com)"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/frmatthew/exsurge/issues"
    },
    "homepage": "https://github.com/frmatthew/exsurge#readme",
    "keywords": [
        "gregorian",
        "chant",
        "square",
        "note",
        "solesmes"
    ],
    "dependencies": {
        "debug": "3.1.0",
        "lodash": "^4.17.15",
        "uuid": "^7.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.4",
        "@babel/node": "7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-decorators": "^7.7.4",
        "@babel/plugin-proposal-export-default-from": "^7.7.4",
        "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
        "@babel/plugin-proposal-numeric-separator": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-proposal-private-methods": "^7.6.0",
        "@babel/plugin-syntax-dynamic-import": "^7.7.4",
        "@babel/plugin-syntax-import-meta": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/register": "^7.7.4",
        "@babel/runtime": "^7.7.4",
        "babel-eslint": "10.1.0",
        "babel-loader": "^8.0.0",
        "babel-plugin-transform-imports": "1.5.1",
        "babel-jest": "^24.9.0",
        "babel-watch": "^7.0.0",
        "eslint": "^6.6.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^23.0.2",
        "jest": "^24.9.0",
        "jest-extended": "^0.11.2",
        "jest-watch-typeahead": "^0.4.0",
        "watch": "^1.0.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "url-loader": "4.1.0",
        "yargs": "^4.1.0"
    }
}
